Package: voronoiTreemap
Type: Package
Title: Voronoi Treemaps with Added Interactivity by Shiny
Version: 0.2.0
Date: 2019-01-08
Authors@R: c(
  person("Alexander", "Kowarik", email = "alexander.kowarik@statistik.gv.at", role = c("aut","cre"), comment=c(ORCID="0000-0001-8598-4130")),
